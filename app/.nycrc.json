{
  "all": true,
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "statements": 0,
  "branches": 0,
  "functions": 0,
  "lines": 0,
  "exclude": [
    "cypress/**/*.*",
    "**/main.tsx",
    "**/utils.ts",
    "**/*.d.ts",
    "**/*.cy.tsx",
    "**/*.cy.ts"
  ]
}
